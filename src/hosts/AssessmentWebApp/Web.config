    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
  </configSections>
  <connectionStrings>
    <add name="CCMEDB" connectionString="{AssessmentWebApp.DatabaseConnectionString}" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <!--DON'T CHECK IN THIS FILE WITH HARD CODE CREDENTIALS-->
    <!--Run $\private\GeneratePrivateConfigFiles.ps1 first.-->
    <!--New credential placeholder should be set in $\private\secrets_mapping.json -->
    <add key="DeploymentId" value="local_debug" />
    <add key="ApplicationId" value="{AssessmentWebApp.ApplicationId}" />
    <add key="ApplicationSecret" value="{AssessmentWebApp.ApplicationSecret}" />
    <add key="ReplyUri" value="http://localhost:50080/" />
    <add key="StorageAccountConnectionString" value="{AssessmentWebApp.StorageAccountConnectionString}" />
    <!--Public Read-Only SAS Connection String (will be expired at 2028-08-01)-->
    <!--[SuppressMessage("Microsoft.Security", "CS002:SecretInNextLine", Justification="Readonly SAS designed for public rule sets")]-->
    <add key="ConfigStorageAccountConnectionString" value="BlobEndpoint=https://ccmeconfigsa.blob.core.windows.net/;TableEndpoint=https://ccmeconfigsa.table.core.windows.net/;SharedAccessSignature=sv=2017-11-09&amp;ss=b&amp;srt=co&amp;sp=rl&amp;se=2028-08-01T00:00:00Z&amp;st=2018-08-01T00:00:00Z&amp;spr=https&amp;sig=PB0%2BCM5IMHE4i94Wj%2BuXsb2r7VQSLvT77CjiJNVODPg%3D" />
    <add key="ApplicationInsightsKey" value="{AssessmentWebApp.ApplicationInsightsKey}" />
  </appSettings>
